{
    "extends": "airbnb",
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "no-use-before-define": ["off", { "classes": false } ],
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off"
    }
}